<hazelcast
        xsi:schemaLocation="http://www.hazelcast.com/schema/config
        http://www.hazelcast.com/schema/config/hazelcast-config-3.8.xsd"
        xmlns="http://www.hazelcast.com/schema/config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <group>
        <name>PRODUCTION</name>
        <password>production-password</password>
    </group>
    <properties>
        <property name="hazelcast.logging.type">slf4j</property>
    </properties>
    <management-center enabled="true" update-interval="3">
        http://localhost:8080/mancenter
    </management-center>
    <network>
        <join>
            <multicast enabled="true"/>
        </join>
    </network>
    <map name="users">
        <map-store enabled="true">
            <class-name >ua.golovchenko.artem.game.dao.UserMapStore</class-name>
        </map-store>
    </map>

<!--    <multimap name="default">
